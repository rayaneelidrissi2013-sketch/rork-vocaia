# Base de données PostgreSQL (Supabase)
DATABASE_URL=postgresql://postgres:password@host:5432/database

# Variables d'environnement optionnelles
NODE_ENV=production
PORT=3000

# Webhook Vapi (optionnel - pour les appels téléphoniques)
VAPI_WEBHOOK_SECRET=your_vapi_webhook_secret_here

# Google Cloud Storage (optionnel - pour le stockage des enregistrements)
GCS_PROJECT_ID=your_project_id
GCS_BUCKET_NAME=your_bucket_name
GCS_KEY_FILE_PATH=path/to/service-account-key.json

# PayPal (optionnel - pour les paiements)
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_MODE=sandbox
